{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["next/core-web-vitals", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": [2, 2],
    "semi": [2, "never"],
    "quotes": ["error", "single"],
    "react/no-children-prop": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  }
}
